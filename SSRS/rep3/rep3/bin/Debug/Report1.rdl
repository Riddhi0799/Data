  <DataSets>
    <DataSet Name="YearData">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct DATEPART(yyyy,soh.DueDate) as [year] from Sales.SalesOrderHeader as soh</CommandText>
      </Query>
      <Fields>
        <Field Name="year">
          <DataField>year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Group">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT distinct [Group] from sales.SalesTerritory</CommandText>
      </Query>
      <Fields>
        <Field Name="Group">
          <DataField>Group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Territory">
            <Value>=Parameters!Territory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select name as C_name from sales.SalesTerritory where [Group] in(@Territory)</CommandText>
      </Query>
      <Fields>
        <Field Name="C_name">
          <DataField>C_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="main">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Territory">
            <Value>=Parameters!Territory.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@YEAR">
            <Value>=Parameters!YEAR.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Country_Name">
            <Value>=Parameters!Country_Name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT cr.Name,DATEPART(yyyy,soh.DueDate) as [Year], soh.SubTotal, soh.Freight, soh.TaxAmt
FROM     Sales.SalesOrderHeader AS soh INNER JOIN
                  Sales.SalesTerritory AS st ON soh.TerritoryID = st.TerritoryID INNER JOIN
                  Sales.SalesOrderDetail AS sod ON soh.SalesOrderID = sod.SalesOrderID INNER JOIN
                  Production.ProductCostHistory AS pch ON sod.ProductID = pch.ProductID INNER JOIN
                  Person.CountryRegion AS cr ON st.CountryRegionCode = cr.CountryRegionCode
WHERE  (st.[Group] IN (@Territory)) AND (DATEPART(yyyy, soh.DueDate) IN (@YEAR)) AND (st.Name IN (@Country_Name))</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SubTotal">
          <DataField>SubTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Freight">
          <DataField>Freight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TaxAmt">
          <DataField>TaxAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
