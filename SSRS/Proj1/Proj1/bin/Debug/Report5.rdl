  <DataSets>
    <DataSet Name="Sales">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Country">
            <Value>=Parameters!Country.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select * from Sales.SalesOrderHeader as soh
join sales.SalesTerritory as st on soh.TerritoryID=st.TerritoryID
join sales.SalesOrderDetail as sod on soh.SalesOrderID=sod.SalesOrderID
join Production.ProductCostHistory as pch on sod.ProductID=pch.ProductID
join Person.CountryRegion as cr on st.countryRegioncode=cr.CountryRegionCode</CommandText>
      </Query>
      <Fields>
        <Field Name="Calendar_year">
          <DataField>Calendar year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sales_territory_group">
          <DataField>sales territory group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales_Territory_Country">
          <DataField>Sales Territory Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales_Territory_Region">
          <DataField>Sales Territory Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales_Amount">
          <DataField>Sales Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Territory">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT [Group]
FROM     Sales.SalesTerritory</CommandText>
      </Query>
      <Fields>
        <Field Name="Group">
          <DataField>Group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Year">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT DATEPART(yyyy, DueDate) AS [calendar year]
FROM     Sales.SalesOrderHeader
ORDER BY [calendar year]</CommandText>
      </Query>
      <Fields>
        <Field Name="calendar_year">
          <DataField>calendar year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT cr.Name AS [sales territory country]
FROM     Person.CountryRegion AS cr INNER JOIN
                  Sales.SalesTerritory AS st ON cr.CountryRegionCode = st.CountryRegionCode</CommandText>
      </Query>
      <Fields>
        <Field Name="sales_territory_country">
          <DataField>sales territory country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
